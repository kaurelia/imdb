{
  "name": "imdb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\" && sort-package-json",
    "lint": "eslint --fix -f unix \"./**/*.{js,jsx,ts,tsx}\"",
    "preview": "vite preview",
    "test": "vitest",
    "build-docker": "docker build . -t imdb",
    "build-development-docker": "docker build . -f dev.Dockerfile -t imdb-development",
    "start-docker": "docker run -p 80:80 -p 443:443 imdb",
    "start-development-image": "docker run -it -p 5173:5173 -p 4173:4173 imdb-development"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/eslint-plugin": "^11.11.0",
